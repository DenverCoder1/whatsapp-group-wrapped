{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.4"
        },
        "api/api.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        { "src": "/", "dest": "/api/index.php" },
        { "src": "/api.php", "dest": "/api/api.php" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "github": {
        "enabled": false
    }
}
